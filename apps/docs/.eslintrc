{
  "rules": {
    "react/react-in-jsx-scope": 0,
    "react/jsx-no-undef": 0,
    "react/prop-types": 0,
    "react/jsx-tag-spacing": 0,
    "no-negated-condition": 0,
    "no-else-return": 0,
    "no-undef": 0,
    "no-unused-expressions": 0,
    "new-cap": 0,
    "react/require-default-props": 0,
    "space-before-function-paren": 0,
    "quotes": 0,
    "no-unused-vars": 0,
    "operator-linebreak": 0,
  }
}